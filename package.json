{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "rm -rf public/build && vite",
        "build": "rm -rf public/build public/hot && vite build",
        "browser": "echo \"const supportedBrowsers = $(browserslist-useragent-regexp --allowHigherVersions);\n\nexport { supportedBrowsers };\" > resources/ts/browser.ts",
        "all:fix": "pnpm run prettier:fix ; pnpm run eslint:fix",
        "eslint": "eslint",
        "eslint:fix": "eslint --fix",
        "prettier": "prettier --check resources/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md} tests/**/*.{css,scss,vue,js,cjs,ts,json,md}",
        "prettier:fix": "prettier --write resources/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md} tests/**/*.{css,scss,vue,js,cjs,ts,json,md}"
    },
    "dependencies": {
        "@eslint/js": "^9.21.0",
        "@primevue/core": "^4.3.1",
        "@sentry/vue": "^9.2.0",
        "@tailwindcss/vite": "^4.0.9",
        "@vitejs/plugin-vue": "^5.2.1",
        "autoprefixer": "^10.4.20",
        "axios": "^1.8.0",
        "concurrently": "^9.1.2",
        "daisyui": "5.0.0-beta.7",
        "laravel-echo": "^2.0.2",
        "laravel-vite-plugin": "^1.2.0",
        "lodash": "^4.17.21",
        "pinia": "^2.3.1",
        "postcss": "^8.5.3",
        "postcss-convert-values": "^7.0.4",
        "primeicons": "^7.0.0",
        "primevue": "^4.3.1",
        "pusher-js": "8.4.0",
        "sass": "^1.85.1",
        "tailwindcss": "^4.0.9",
        "tailwindcss-primeui": "^0.4.0",
        "vite": "^6.2.0",
        "vite-plugin-static-copy": "^2.2.0",
        "vue": "^3.5.13",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.8",
        "browserslist-useragent-regexp": "^4.1.3",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-vue": "^9.32.0",
        "globals": "^15.15.0",
        "prettier": "^3.5.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.25.0"
    }
}
