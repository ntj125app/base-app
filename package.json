{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "rm -rf public/build && vite",
        "build": "rm -rf public/build public/hot && vite build",
        "browser": "echo \"const supportedBrowsers = $(browserslist-useragent-regexp --allowHigherVersions);\n\nexport { supportedBrowsers };\" > resources/ts/browser.ts",
        "all:fix": "pnpm run prettier:fix ; pnpm run eslint:fix",
        "eslint": "eslint",
        "eslint:fix": "eslint --fix",
        "prettier": "prettier --check resources/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md} tests/**/*.{css,scss,vue,js,cjs,ts,json,md}",
        "prettier:fix": "prettier --write resources/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md} tests/**/*.{css,scss,vue,js,cjs,ts,json,md}"
    },
    "dependencies": {
        "@eslint/js": "^9.23.0",
        "@nuxt/ui": "^3.0.1",
        "@primevue/core": "^4.3.2",
        "@sentry/vue": "^9.9.0",
        "@tailwindcss/vite": "^4.0.16",
        "@unhead/vue": "^2.0.1",
        "@vitejs/plugin-vue": "^5.2.3",
        "autoprefixer": "^10.4.21",
        "axios": "^1.8.4",
        "concurrently": "^9.1.2",
        "daisyui": "^5.0.9",
        "laravel-echo": "^2.0.2",
        "laravel-vite-plugin": "^1.2.0",
        "lodash": "^4.17.21",
        "pinia": "^3.0.1",
        "postcss": "^8.5.3",
        "postcss-convert-values": "^7.0.4",
        "primeicons": "^7.0.0",
        "primevue": "^4.3.2",
        "pusher-js": "8.4.0",
        "sass": "^1.86.0",
        "tailwindcss": "^4.0.16",
        "tailwindcss-primeui": "^0.6.0",
        "vite": "^6.2.3",
        "vite-plugin-static-copy": "^2.3.0",
        "vue": "^3.5.13",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.8",
        "browserslist-useragent-regexp": "^4.1.3",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-prettier": "^5.2.5",
        "eslint-plugin-vue": "^9.33.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0"
    }
